  },
  "homepage": "https://github.com/sommyj/We-Connect#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "express": "^4.16.3",
    "morgan": "^1.9.0",
    "n": "^2.1.12"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "coveralls": "^3.0.1",
    "eslint": "^4.19.1",
